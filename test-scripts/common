#!/bin/bash

ATEST="$(dirname "$(realpath "$0")")/../atest"
export ATEST

FXSEND="$(dirname "$(realpath "$0")")/../fxsend"
export FXSEND
FXREC="$(dirname "$(realpath "$0")")/../fxrec"
export FXREC

GEN_PACKETS="$(dirname "$(realpath "$0")")/../gen_packets"
export GEN_PACKETS

function goto_tmpdir() {
	TMPDIR=$(mktemp --directory)
	echo "$TMPDIR"
	cd "$TMPDIR" || exit
}
